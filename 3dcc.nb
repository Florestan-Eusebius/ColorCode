               "\[IndentingNewLine]", 
               RowBox[{"vertexJ", "=", 
                RowBox[{"vertices", "[", 
                 RowBox[{"[", 
                  RowBox[{"2", ",", "j"}], "]"}], "]"}]}], ";", 
               "\[IndentingNewLine]", 
               RowBox[{"vertexK", "=", 
                RowBox[{"vertices", "[", 
                 RowBox[{"[", 
