          RowBox[{"Do", "[", 
           RowBox[{
            RowBox[{"Do", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"vertexI", "=", 
                RowBox[{"vertices", "[", 
                 RowBox[{"[", 
