       RowBox[{
        RowBox[{"Do", "[", 
         RowBox[{
          RowBox[{"Do", "[", 
           RowBox[{
            RowBox[{"Do", "[", 
             RowBox[{
              RowBox[{"AppendTo", "[", 
               RowBox[{"vertices", ",", 
                RowBox[{"origin", "+", 
                 RowBox[{"x", " ", 
                  RowBox[{"basis", "[", 
                   RowBox[{"[", "1", "]"}], "]"}]}], "+", 
                 RowBox[{"y", " ", 
                  RowBox[{"basis", "[", 
                   RowBox[{"[", "2", "]"}], "]"}]}], "+", 
                 RowBox[{"z", " ", 
                  RowBox[{"basis", "[", 
                   RowBox[{"[", "3", "]"}], "]"}]}]}]}], "]"}], ",", 
              RowBox[{"{", 
               RowBox[{"z", ",", "0", ",", 
                RowBox[{"k", "-", "x", "-", "y", "-", "1"}]}], "}"}]}], "]"}],
             ",", 
            RowBox[{"{", 
             RowBox[{"y", ",", "0", ",", 
              RowBox[{"k", "-", "x", "-", "1"}]}], "}"}]}], "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"x", ",", "0", ",", 
            RowBox[{"k", "-", "1"}]}], "}"}]}], "]"}], ";", 
        "\[IndentingNewLine]", "vertices"}]}], "]"}]}], ";"}], "\n", 
   "\[IndentingNewLine]", 
